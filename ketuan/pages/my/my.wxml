<view class="container1">
  <view class='maintitle'>我的客团</view>

  <view class="userinfo">
		<image class="userinfo-avatar" src="{{ userInfo.avatarUrl }}" background-size="cover"></image>
		<text class="userinfo-nickname">{{userInfo.nickName}}</text>
    <text class='userinfo-manage'>账号管理>></text>
	</view>

  <view class='order-item'>
    <text class='order-sub'>我的订单</text>
    <text class='order-more'>更多>></text>
    <view class="iconimg0">
       <image  src='{{iconimg[0]}}'></image>
       <text>待支付</text>
    </view>
    <view class="iconimg1">
       <image  src='{{iconimg[1]}}'></image>
       <text>待发货</text>
    </view>
    <view class="iconimg2">
       <image  src='{{iconimg[2]}}'></image>
       <text>待收货</text>
    </view>
    <view class="iconimg3">
       <image  src='{{iconimg[3]}}'></image>
       <text>待评价</text>
    </view>
    <view class="iconimg4">
       <image  src='{{iconimg[4]}}'></image>
       <text>退换货</text>
    </view>



  </view>

  <view class='address-item'>
    <text class='sub-myaddress'>我的地址</text>
    <text class='sub-address-manage'>管理>></text>
    <view class='sub-address'>{{address}}</view>
  </view>

  <view class='count-item'>
    <text class='sub-mycount'>我的客团账户</text>
    <view class='subitem-count'>
    <text class='sub-gradetext'>账户等级</text>
    <text class='sub-grade'>等级</text>
    </view>
    <view class='subitem-back'>
    <text class='sub-backtext'>上月返现</text>
    <text class='sub-back'>￥160</text>
    <text class='sub-back-more'>更多</text>
    </view>
    <view class='subitem-recommend'>
    <text class='sub-rectext'>我的推荐</text>
      <view class='sub-rec'>暂无</view>
      <text class='sub-rec-more'>更多</text>
    </view>
    <text class='sub-share'>分享个人名片</text>
  </view>
</view>