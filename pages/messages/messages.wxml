<!--pages/message/messages.wxml-->

<view>
  <view class='list-container' wx:for="{{msglist}}" bindtap='gototalk' data-userid='{{item.userid}}'>
   <image class='avater' src='{{item.avatar}}'  ></image>
   <text class='nickname' bindtap='express'>{{item.nickname}}</text>
   <text class='message'>{{item.message}}</text>
   <image class="{{item.is_new==true?'newmsg-img':'none'}}" src='/images/icon/newmsg.png'></image>
  </view>
</view>